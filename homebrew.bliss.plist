<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.vyrtsev.tmbliss</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/tmbliss</string>
		<string>conf</string>
		<string>--path</string>
		<string>/usr/local/etc/tmbliss.conf.json</string>
	</array>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>10</integer>
		<key>Minute</key>
		<integer>0</integer>
	</dict>
	<key>StandardOutPath</key>
	<string>/tmp/tmbliss.log</string>
	<key>StandardErrorPath</key>
	<string>/tmp/tmbliss.err.log</string>
</dict>
</plist>